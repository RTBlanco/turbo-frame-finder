{
  "manifest_version": 3,

  "name": "Turbo-Frame Finder",
  "description": "Highlights the turbo-frames in a rails poject",
  "version": "1.0.0",
  "icons": { 
    "128": "" 
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"],
  "content_scripts": [{
    "matches": ["http://127.0.0.1:3000/*"],
    "css": ["main.css"]
  }]
}
